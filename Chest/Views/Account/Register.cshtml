@model Chest.ViewModels.RegisterViewModel



<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div class="centerDiv">
    <h2>New user registration</h2>
    <form method="post" asp-controller="Account" asp-action="Register">
        <div class="form-group">
            <label>Email</label>
            <input class="formTextField" type="text" name="email"/>
        </div>
        <div class="form-group">
            <label>Password</label>
            <input class="formTextField" type="text" name="password" />
        </div>
        <div class="form-group">
            <input class="formSubmitButton" type="submit" value="Registration" />
        </div>
    </form>
</div>
</body>
</html>